<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
  <!-- Suppress false positives and known issues -->

  <!-- Node.js ecosystem aliases that confuse dependency-check -->
  <suppress>
    <notes><![CDATA[string-width-cjs is an alias package, not a security concern]]></notes>
    <packageUrl regex="true">^pkg:npm/string-width-cjs@.*$</packageUrl>
    <cpe>cpe:/a:*:*</cpe>
  </suppress>

  <suppress>
    <notes><![CDATA[strip-ansi-cjs is an alias package, not a security concern]]></notes>
    <packageUrl regex="true">^pkg:npm/strip-ansi-cjs@.*$</packageUrl>
    <cpe>cpe:/a:*:*</cpe>
  </suppress>

  <suppress>
    <notes><![CDATA[wrap-ansi-cjs is an alias package, not a security concern]]></notes>
    <packageUrl regex="true">^pkg:npm/wrap-ansi-cjs@.*$</packageUrl>
    <cpe>cpe:/a:*:*</cpe>
  </suppress>
</suppressions>
